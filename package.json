{
  "name": "OVALE",
  "author": "John Thillaye <john@jaytee.club>",
  "description": "Local multi-exchange trading client. Supports Bitfinex, Bittrex, Poloniex.",
  "productName": "OVALE",
  "version": "1.0.0",
  "main": "main/index.js",
  "license": "MIT",
  "repository": "johnthillaye/cryptotrader-app",
  "scripts": {
    "start": "electron .",
    "reset": "electron . --reset",
    "build": "next build renderer && next export renderer",
    "dist": "npm run build && build --dir",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "bundle": {
    "name": "OVALE"
  },
  "build": {
    "appId": "org.jaytee.ovale",
    "mac": {
      "target": "dmg",
      "category": "public.app-category.finance",
      "icon": "main/assets/icons/mac/icon.icns"
    },
    "files": [
      "**/*",
      "!renderer",
      "renderer/out"
    ],
    "asarUnpack": [
      "**/renderer/out/static/**/*"
    ],
    "win": {
      "target": "NSIS",
      "icon": "main/assets/icons/win/icon.ico"
    },
    "linux": {
      "category": "Finance",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "devDependencies": {
    "babel-plugin-inline-import": "^2.0.6",
    "electron": "1.7.5",
    "electron-builder": "19.22.1",
    "electron-packager": "^10.1.0",
    "next": "^4.0",
    "react": "^16.0",
    "react-dom": "^16.0",
    "react-hot-loader": "^4.0.0-beta.18",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "@storybook/addon-info": "^3.4.8",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "async": "^2.6.0",
    "cryptoclients": "^1.0.26",
    "css-loader": "^0.28.9",
    "electron-context-menu": "^0.9.1",
    "electron-debug": "^1.5.0",
    "electron-is-dev": "^0.3.0",
    "electron-next": "^3.1.3",
    "electron-settings": "^3.1.4",
    "express": "^4.16.2",
    "history": "^4.7.2",
    "material-ui": "^0.20.0",
    "next-redux-wrapper": "^1.3.5",
    "react-interval": "^2.0.2",
    "react-moment": "^0.6.9",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "request": "^2.83.0",
    "reselect": "^3.0.1",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "style-loader": "^0.19.1"
  }
}
